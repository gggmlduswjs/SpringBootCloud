<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="a">

    <resultMap id="member2Map" type="Member2">
        <result property="m_id" column="id"/>
        <result property="m_name" column="name"/>
        <result property="m_email" column="email"/>
    </resultMap>



    <select id="SelectAll2" resultMap="member2Map">
        select   *  from  membertbl
    </select>



    <select id="SelectAll" resultType="Member">
        select   *  from  membertbl
    </select>

    <select id="SelectAll2" resultMap="member2Map">
        select   *  from  membertbl
    </select>



    <insert id="insert"  parameterType="Member">
        INSERT INTO membertbl ( id , name, email )
        VALUES( #{id} ,  #{name} , #{email})

    </insert>

</mapper>